{% if events is not empty %}
    <h2>Events</h2>

    <ul>
        {% for event in events %}
            {% include 'talk/events/event.html.twig' with {
                date: event.date,
                is_online: event.is_online ?? false,
                location: event.location,
                name: event.name,
                url: event.url,
            } only %}
        {% endfor %}
    </ul>
{% endif %}
