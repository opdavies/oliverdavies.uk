{
    "require": {
        "illuminate/collections": "^11.6",
        "opdavies/sculpin-twig-markdown-bundle": "^0.2.0",
        "sculpin/sculpin": "^3.2"
    },
    "config": {
        "allow-plugins": {
            "sculpin/sculpin-theme-composer-plugin": true
       }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src" }
   },
   "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests" }
   },
    "require-dev": {
        "phpunit/phpunit": "^11.1"
    }
}
